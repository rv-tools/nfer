// nfer 2:

IVAL :- BEGIN before END
  where BEGIN.interval = END.interval
  map {interval -> BEGIN.interval, data -> BEGIN.data}

EXEC :- i1:IVAL before i2:IVAL
  where (i1.data = "DIS_MOB_PRM" & i2.data = "COM_MOB_PRM") |
        (i1.data = "DIS_ARM_PRM" & i2.data = "COM_ARM_PRM")

ERROR :- IVAL where IVAL.data = "TLM_TR_ERROR"

RACE :- ERROR during EXEC
